<% if(type == 'normal') { %>
	<exclude-tag>
		<style type="text/css">
			.pdf-new-window-box {
				display: flex;
				justify-content: flex-end;
				align-items: center;
				color: #666;
			
			}
			.pdf-new-window-box small {
					font-size: 12px;
					color: #999;
				}
			.pdf-new-window-box a {
				color: #999;
			}
		</style>
		<div class="pdf-new-window-box">
			<div>
				<a href="<%= src %>" target="_blank">新窗口打开</a>
				<small>可放大文本</small>
			</div>
		</div>
	</exclude-tag>
	<div class="row pdf-viewer-tel" id="pdfViewerTeleport">
		<style type="text/css">
			#viewerContainer .pdfViewer .page {
				margin: 0 0 0 -1.5em;
			}
			#cutomeLoadingBar {
				width: 100%;
				height: 28px;
				border-radius: 8px;
				overflow: hidden;
				z-index: 99;
			}
			#cutomeLoadingBar .progress {
				height: 100%;
				background-color: #3da742;
				color: #fff;
				font-size: 14px;
				font-weight: 500;
				text-indent: 20px;
				transition: all 0.2s ease-in;
			}
		</style>
		<link rel="stylesheet" href="/js/pdfjs-dist@2.8.335/web/pdf_viewer.css">
		<script src="/js/pdfjs-dist@2.8.335/build/pdf.min.js"></script>
		<script src="/js/pdfjs-dist@2.8.335/web/pdf_viewer.js"></script>
		<div id="cutomeLoadingBar">
      <div class="progress" style="width: 0%;"></div>
      <div class="glimmer"></div>
    </div>
		<div id="viewerContainer" style="width: 100%; height: 100%; position:absolute; overflow: hidden;">
			<div id="viewer" class="pdfViewer">
			</div>
		</div>

		<script src="/js/viewer.js"></script>
		<script>
			window.embedPDF("<%= src %>")
		</script>
	</div>


<% } else if (type == 'googledoc'){ %>

	<div class="row">
		<iframe src="<%= src %>" style="width:100%; height:550px"></iframe>
	</div>

<% } else if(type == 'slideshare') { %>
	<iframe src="<%= src %>" style="width:100%;height:550px" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" style="border:1px solid #CCC; border-width:1px; margin-bottom:5px; max-width: 100%;" allowfullscreen> </iframe> <div style="margin-bottom:5px">
<% } %>
