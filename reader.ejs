<% if(type == 'normal') { %>
	<exclude-tag>
		<style type="text/css">
			.pdf-new-window-box {
				display: flex;
				justify-content: flex-end;
				align-items: center;
				color: #666;
			}
			.pdf-new-window-box > a {
				color: #999;
			}
		</style>
		<div class="pdf-new-window-box">
			<a href="<%= src %>" target="_blank">新窗口打开</a>
		</div>
	</exclude-tag>
	<div class="row pdf-viewer-tel" id="pdfViewerTeleport">
		<style type="text/css">
			#viewerContainer .pdfViewer .page {
				margin: 0 0 0 -1.5em;
			}
			#cutomeLoadingBar {
				width: 100%;
				height: 28px;
				border-radius: 8px;
				overflow: hidden;
			}
			#cutomeLoadingBar .progress {
				height: 100%;
				background-color: #3da742;
				color: #fff;
				font-size: 14px;
				font-weight: 500;
				text-indent: 20px;
				transition: all 0.2s ease-in;
			}
		</style>
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.8.335/pdf_viewer.css" integrity="sha512-k/Xe8dvVy8g0sUMaPGV+23j64J+0HvD9abad6LshzjQSNXX2+WUFuGS5B+iZfGzWqhZhM1nlZ12sq1Sfe/C2eQ==" crossorigin="anonymous" />
		<script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.8.335/pdf.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.8.335/pdf_viewer.js" integrity="sha512-O6aDht9IROQup2Ag1xf8F87x34eEzI5ZjNRvt8qx/itgTuAj2Bu41kZafLn/RAnH6fK7AfAFCrcAbjSqXUSQyQ==" crossorigin="anonymous"></script>
		
		<div id="viewerContainer" style="width: 100%; height: 100%; position:absolute; overflow: hidden;">
			<div id="viewer" class="pdfViewer">
			</div>
		</div>

		<div id="cutomeLoadingBar">
      <div class="progress" style="width: 0%;"></div>
      <div class="glimmer"></div>
    </div>

		<script src="/js/viewer.js"></script>
		<script>
			window.embedPDF("<%= src %>")
		</script>
	</div>


<% } else if (type == 'googledoc'){ %>

	<div class="row">
		<iframe src="<%= src %>" style="width:100%; height:550px"></iframe>
	</div>

<% } else if(type == 'slideshare') { %>
	<iframe src="<%= src %>" style="width:100%;height:550px" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" style="border:1px solid #CCC; border-width:1px; margin-bottom:5px; max-width: 100%;" allowfullscreen> </iframe> <div style="margin-bottom:5px">
<% } %>
